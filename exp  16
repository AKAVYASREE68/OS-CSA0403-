#include <stdio.h>

int main() {
    int block[3] = {100, 500, 200};
    int process[3] = {90, 250, 120};
    int i, j;

    for (i = 0; i < 3; i++) {
        for (j = 0; j < 3; j++) {
            if (block[j] >= process[i]) {
                printf("Process %d allocated to Block %d\n", i + 1, j + 1);
                block[j] = block[j] - process[i];
                break;
            }
        }
        if (j == 3)
            printf("Process %d not allocated\n", i + 1);
    }
    return 0;
}
