#include <stdio.h>
#include <stdlib.h>

int main() {
    int req[4] = {98, 183, 37, 122};
    int head = 53;
    int i, total = 0;

    for (i = 0; i < 4; i++) {
        total = total + abs(req[i] - head);
        head = req[i];
    }

    printf("Total head movement = %d\n", total);
    return 0;
}
