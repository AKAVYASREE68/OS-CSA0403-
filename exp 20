#include <stdio.h>

int main() {
    char user1[2][20] = {"a.txt", "b.txt"};
    char user2[2][20] = {"c.txt", "d.txt"};
    int i;

    printf("User1 files:\n");
    for (i = 0; i < 2; i++)
        printf("%s\n", user1[i]);

    printf("User2 files:\n");
    for (i = 0; i < 2; i++)
        printf("%s\n", user2[i]);

    return 0;
}
